@article{vali2018,
  title = {Genetic Determination of Migration Strategies in Large Soaring Birds: Evidence from Hybrid Eagles},
  shorttitle = {Genetic Determination of Migration Strategies in Large Soaring Birds},
  author = {V{\"a}li, {\"U}lo and Mirski, Pawe{\l} and Sellis, Urmas and Dagys, Mindaugas and Maciorowski, Grzegorz},
