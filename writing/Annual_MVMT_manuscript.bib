@article{henry2016a,
  title = {Exploring the Environmental Drivers of Waterfowl Movement in Arid Landscapes Using First-Passage Time Analysis},
  author = {Henry, Dominic A. W. and Ament, Judith M. and Cumming, Graeme S.},
